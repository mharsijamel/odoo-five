<?xml version="1.0" encoding="UTF-8"?>

<odoo>
     <record model="ir.ui.view" id="document_treasury_unpaid_wizard">
        <field name="name">document.treasury.unpaid.form</field>
        <field name="model">document.treasury.unpaid</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group string="Detail">
                        <field name="check_id" readonly="1"/>
                        <field name="partner_id" readonly="1"/>
                    </group>
                    <group>
                        <field name="amount" readonly="1"/>
                        <field name="company_id" readonly="1" groups="base.group_multi_company"/>
                    </group>
                </group>
                <footer>
                    <button name="button_confirm" string="confirm" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>