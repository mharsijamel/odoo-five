<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="chauf_voiture.picking" model="ir.ui.view">
        <field name="name">chauf_voiture.picking</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
                <data><xpath expr="//field[@name='location_id']" position="after">
						<field name="x_chaffeur"/>
						<field name="x_voiture"/>
					</xpath>
				</data>
        </field>
    </record>
</odoo>
