                        <field name="move_type" invisible="1"/>
                         <button name="action_rs"  type="object" class="oe_highlight" string="Retenue à la Source" attrs="{'invisible':[('withholding_id','not in',['',None,False])]}"  />
<!--                         <button name="action_rs"  type="object" class="oe_highlight" string="Retenue à la Source" attrs="{'invisible':['|',('amount','&lt;',1000),('has_withholding','!=',False)]}"   />-->
                    </xpath>
            </field>
        </record>
    </data>
</odoo>