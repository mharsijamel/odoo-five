<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

		<record id="action_print_check_biatta" model="ir.actions.report">
            <field name="name">Print Check (biatta)</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_tn_check_printing.print_check_biatta</field>
            <field name="report_file">l10n_tn_check_printing.print_check_biatta</field>
            <field name="print_report_name">'Check biatta - %s' % (object.partner_id.name or '',)</field>
            <field name="attachment">'check_'+(object.partner_id and object.partner_id.name or 'Internal')+'_'+(object.name)+'_biatta.pdf'</field>
            <field name="attachment_use">True</field>
        </record>

    </data>
</odoo>
