<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
<template id="ckus_checks">
    <div class="ckus_checks">
        <t t-if="page['state'] == 'cancelled'">
            <div class="o-watermark">VOID</div>
        </t>
        <div class="ckus_date" style="top: 2cm !important; left: 6.51cm !important; width: 1.47cm !important; text-align: center !important;" t-esc="page['date']"/>
        <div class="ckus_payee_name" style="top: 1.07cm !important; left: 6.7cm !important; width: 1.08cm !important; text-align: left !important;" t-esc="page['partner_name']"/>
	    <div style="top: 1.45cm !important; left: 0.25cm !important; width: 7cm !important; text-align: left !important; font-weight: bold !important; overflow: hidden !important;" t-esc="page['amount_in_word']"/>
    </div>
</template>

</data>
</odoo>
